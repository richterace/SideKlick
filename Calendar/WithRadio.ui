/* Calendar Navigation Bar */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #95b87f; /* Grey background color */
	color: rgb(149, 184, 127);
}

/* Calendar Navigation Buttons */
QCalendarWidget QToolButton {
    color: #95b87f; /* Text color */
    border: 1px solid #dcdcdc; /* Border style */
    border-radius: 10px; /* Rounded corners */
    padding: 10px; /* Padding */
}

/* Calendar Month and Year */
QCalendarWidget QSpinBox, QCalendarWidget QComboBox {
    color: #d1e6bf; /* Text color */
    padding: 5px; /* Adjust padding for year */
    border: 4px solid #dcdcdc; /* Border style */
    border-radius: 10px; /* Rounded corners */
}

/* Remove down-arrow indication */
QCalendarWidget QComboBox::down-arrow {
    width: 0;
    color: transparent;
}

/* Calendar Dropdown */
QCalendarWidget QMenu {
    width: 200px; /* Width of the dropdown */
}

/* Selected Date */
QCalendarWidget QAbstractItemView:enabled {
    selection-background-color: #95b87f; /* Background color of selected date */
	border-radius:5px;
    selection-color: #ffffff; /* Text color of selected date */
}

/* Disabled Dates */
QCalendarWidget QAbstractItemView:disabled {
    color: #808080; /* Disabled text color */
}



